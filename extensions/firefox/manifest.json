{
    "manifest_version": 2,
    "name": "NVIDIA nvis",
    "description": "A image/stream viewer extension!",
    "version": "0.4",
    "icons": {
        "16": "icons/nvis16.png",
        "32": "icons/nvis32.png",
        "48": "icons/nvis48.png",
        "128": "icons/nvis128.png"
    },
    "background": {
        "scripts": [
            "content.js"
        ]
    },
    "browser_action": {
        "default_icon": {
            "16": "icons/nvis16.png",
            "32": "icons/nvis32.png",
            "48": "icons/nvis48.png",
            "128": "icons/nvis128.png"
        },
        "default_title": "Click to start nvis!"
    }
}