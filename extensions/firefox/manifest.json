{
    "name": "NVIDIA nvis",
    "description": "An image/stream viewer extension by NVIDIA!",
    "version": "0.7",
    "manifest_version": 2,
    "icons": {
        "16": "icons/nvis-logo-16.png",
        "32": "icons/nvis-logo-32.png",
        "48": "icons/nvis-logo-48.png",
        "128": "icons/nvis-logo-128.png"
    },
    "background": {
        "scripts": [
            "content.js"
        ]
    },
    "browser_action": {
        "default_icon": {
            "16": "icons/nvis-logo-16.png",
            "32": "icons/nvis-logo-32.png",
            "48": "icons/nvis-logo-48.png",
            "128": "icons/nvis-logo-128.png"
        },
        "default_title": "Click to start nvis!"
    }
}